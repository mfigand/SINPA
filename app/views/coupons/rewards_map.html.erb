<h2 class="title is-2">Rewards near your location</h2>
<br>

<div id="rewards_map2"></div>

<ul>
  <% @branches.each_with_index do |branch, index| %>
    <li><span class="js-rewards-lat" data-rewards-lat='<%= "#{branch.latitude}" %>'></span></li>
    <li><span class="js-rewards-long" data-rewards-long='<%= "#{branch.longitude}" %>'></span></li>
    <br>
  <% end %>
</ul>
<br>

<a href="<%= user_path(current_user)%>">Back to Profile</a>
